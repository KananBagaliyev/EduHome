@model BackEndProject.Areas.Admin.ViewModels.ContactVM
<!-- Teacher Start -->
<div class="teacher-details-area pt-150 pb-60">
    <div class="container">
        <div class="row">
            <div class="col-md-5 col-sm-5 col-xs-12">
                <div class="teacher-details-img">
                    <img src="~/img/logo/@Model.Data.Logo" alt="teacher">
                </div>
            </div>
            <div class="col-md-7 col-sm-7 col-xs-12">
                <div class="teacher-details-content ml-50">
                    <ul>
                        <li><span>Location: </span>@Model.Data.Location</li>
                        <li><span>Mail: </span>@Model.Data.Mail</li>
                        <li><span>Web: </span>@Model.Data.Web</li>
                        <li>
                            <span>Numbers: </span>
                            @foreach (CenterPhoneNumber cpn in Model.Numbers)
                            {
                                <p>@cpn.Phone</p>
                            }
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-3 col-sm-4">
                <div class="teacher-contact">
                    <ul style="list-style:none;float:left">
                        <li><a target="_blank" style="float:left" href="@@Model.ElementAt(0).Data.Facebook"><i class=" mdi mdi-facebook "></i></a></li>
                        <li><a target="_blank" href="@Model.Data.Pinterest"><i class="mdi mdi-pinterest"></i></a></li>
                        <li><a target="_blank" href="@Model.Data.Vimeo"><i class="mdi mdi-vimeo"></i></a></li>
                        <li><a target="_blank" href="@Model.Data.Twitter"><i class="mdi mdi-twitter"></i></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Teacher End -->
